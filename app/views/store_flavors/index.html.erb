  <style>
    body { background-color: #FFCFCA; }
  </style>
<div class="container">
<p id="notice"><%= notice %></p>
  <div class="small-1 columns">&nbsp;</div>
	<div class="small-5 columns">&nbsp;</div>
<h1>List of Store Flavor</h1>

<%= link_to 'New Store Flavor', new_shift_path, :class=> "right" %><br>
<table class="striped">
  <thead>
    <tr>
      <th>Store</th>
      <th>Flavor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @store_flavors.each do |store_flavor| %>
      <tr>
        <td><%= store_flavor.store_id %></td>
        <td><%= store_flavor.flavor_id %></td>
        <td><%= link_to 'Show', store_flavor %></td>
        <td><%= link_to 'Edit', edit_store_flavor_path(store_flavor) %></td>
        <td><%= link_to 'Destroy', store_flavor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>


<br>

  <div class="small-1 columns">&nbsp;</div>
	<div class="small-5 columns">&nbsp;</div>
</div>
